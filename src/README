CC=g++
CFLAGS=-c -Wall
LDFLAGS=
SOURCES=problem_01.cpp
OBJECTS=$(SOURCES:.cpp=.o)
EXECUTABLE=problem_01

all: $(SOURCES) $(EXECUTABLE)
	
$(EXECUTABLE): $(OBJECTS) 
	$(CC) $(LDFLAGS) $(OBJECTS) -o $@

.cpp.o:
	$(CC) $(CFLAGS) $< -o $@